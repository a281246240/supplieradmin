<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
 	
 	<rule> 
		<from>^/(.*).html$</from>
		<to>/$1.jsp</to>
	</rule>  
 	<rule> 
		<from>^/(.*).htm$</from>
		<to>/$1.jsp</to>
	</rule>  
 	<rule> 
		<from>^/(.*).asp$</from>
		<to>/$1.jsp</to>
	</rule>
	
	<outbound-rule encodefirst="true">
        <from>^(.*);jsessionid=.*$</from>
        <to>$1</to>
    </outbound-rule>

    <outbound-rule match-type="wildcard">
        <from>*;jsessionid=*</from>
        <to>$1</to>
    </outbound-rule> 
	
</urlrewrite>